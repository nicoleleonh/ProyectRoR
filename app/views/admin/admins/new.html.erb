


    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div>
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Administrators</h1>
      </div>

      <%= form_for :admin, url: admin_admins_path do |f| %>
        <div class="mb-3">
          <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "name@example.com", class: "form-control" %>
          <%= f.label :email %>
        </div>

        <div class="mb-3">
          <%= f.password_field :password, autocomplete: "new-password", class: "form-control" %>
          <%= f.label :password %>
        </div>

        <%= f.submit "Sign in", class: "btn btn-sm btn-outline-secondary" %>
      <% end %>
      
    </main>