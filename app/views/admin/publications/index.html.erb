


    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div>
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Publications</h1>
      </div>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Age</th>
              <th scope="col">Amount to raise</th>
              <th scope="col">Amount to collected</th>
              <th scope="col">User ID</th>
            </tr>
          </thead>
          <tbody>
          <% @publication.each do |p| %>
            <tr>
              <td><%= p.name %></td>
              <td><%= p.age %></td>
              <td><%= p.amountoraise %></td>
              <td><%= p.confirmed_donations_amount %></td>
              <td><%= p.user_id %></td>
              <td><%= link_to 'Edit', edit_admin_publication_path(p) %></td>
              <!-- <td><%= link_to 'Destroy', admin_publication_path(p), method: :delete, data: { confirm: 'Are you sure?' } %></td>       -->
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </main>